{
  "enabledApiProposals": [
    "externalUriOpener"
  ],
  "name": "hypr-cseb",
  "displayName": "hypr-cseb",
  "description": "Customed simple embedded browser for hypers",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hyprCseb.show",
        "title": "Hello Browser"
      }
    ]
  },
  "configuration": [
    {
      "title": "Hello Browser",
      "properties": {
        "hyprCseb.focusLockIndicator.enabled": {
          "type": "boolean",
          "default": true,
          "title": "Focus Lock Indicator Enabled",
          "description": "%configuration.focusLockIndicator.enabled.description%"
        }
      }
    }
  ],
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build-preview": "node ./esbuild-preview"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@vscode/test-electron": "^2.1.5",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "vscode-dts": "^0.3.3"
  },
  "dependencies": {
    "esbuild": "^0.15.12",
    "gulp": "^4.0.2",
    "vscode-codicons": "^0.0.17",
    "vscode-nls": "^5.2.0"
  }
}
