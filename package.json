{
  "enabledApiProposals": [
    "externalUriOpener"
  ],
  "name": "hypr-cseb",
  "displayName": "hypr-cseb",
  "description": "Customed simple embedded browser for hypers",
  "version": "2.5.0",
  "engines": {
    "vscode": "^1.70.0"
  },
  "icon": "media/tuning.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:hyprCseb.show",
    "onCommand:hyprCseb.api.open",
    "onOpenExternalUri:https",
    "onWebviewPanel:hyprCseb.view"
  ],
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hyprCseb.show",
        "title": "Hello Browser"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "kunpengTuning",
          "title": "kunpeng hyper tuner plugin",
          "icon": "media/tuning.svg"
        }
      ]
    },
    "views": {
      "kunpengTuning": [
        {
          "id": "tuning-dependencies",
          "name": "Dependencies"
        },
        {
          "id": "tuning-outline",
          "name": "Outline"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build-preview": "node ./esbuild-preview"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "typescript": "^4.8.4",
    "esbuild": "^0.15.12",
    "vscode-dts": "^0.3.3"
  },
  "dependencies": {
    "vscode-nls": "^5.2.0"
  }
}
